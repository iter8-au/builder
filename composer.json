      "email": "dean@iter8.com.au",
      "homepage": "https://iter8.com.au"
    }
  ],
  "require": {
    "php": "^5.4",
    "phpoffice/phpexcel": "~1.8",
    "box/spout": "^2.7",
    "roave/security-advisories": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "silex/silex": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Builder\\": "src/Builder"
    }
  }
