{
    "name": "iter8/builder",
    "type": "library",
    "description": "A wrapper for PHP Excel libraries to help you quickly build reports",
    "keywords": [
        "phpoffice",
        "phpspreadsheet",
        "excel",
        "spout"
    ],
    "homepage": "https://github.com/iter8-au/builder",
    "license": "MIT",
    "authors": [
        {
            "name": "Dean Smith",
            "email": "dean@iter8.com.au",
            "homepage": "https://iter8.com.au"
        },
        {
            "name": "Dan Barrett",
            "email": "dan@iter8.com.au",
            "homepage": "https://iter8.com.au"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "box/spout": "^3.1",
        "phpoffice/phpspreadsheet": "^1.16",
        "webmozart/assert": "^1.9"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.13",
        "friendsofphp/php-cs-fixer": "^2.18",
        "phpunit/phpunit": "^8.0",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-deprecation-rules": "^0.12",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpstan/phpstan-strict-rules": "^0.12"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Iter8\\Builder\\": "src/Builder"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Iter8\\Builder\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev"
}
