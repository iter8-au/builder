{
  "name": "iter8/builder",
  "version": "1.0.0",
  "description": "A wrapper for the PHP Excel library to help you quickly build reports",
  "keywords": ["phpexcel", "reports", "excel", "spout"],
  "homepage": "https://github.com/iter8-au/builder",
  "type": "library",
  "authors": [
    {
